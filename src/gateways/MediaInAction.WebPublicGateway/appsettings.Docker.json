{  
  "ReverseProxy": {
    "Routes": {
      "Account Service": {
        "ClusterId": "accountCluster",
        "Match": {
          "Path": "/api/account/{**everything}"
        }
      },
      "Administration Service": {
        "ClusterId": "administrationCluster",
        "Match": {
          "Path": "/api/abp/{**everything}"
        }
      },
      "File Service": {
        "ClusterId": "fileCluster",
        "Match": {
          "Path": "/api/file/{**everything}"
        }
      },
      "Deluge Service": {
        "ClusterId": "delugeCluster",
        "Match": {
          "Path": "/api/deluge/{**everything}"
        }
      },
      "Emby Service": {
        "ClusterId": "embyCluster",
        "Match": {
          "Path": "/api/emby/{**everything}"
        }
      },
      "Video Service": {
        "ClusterId": "videoCluster",
        "Match": {
          "Path": "/api/video/{**everything}"
        }
      },
      "Trakt Service": {
        "ClusterId": "traktCluster",
        "Match": {
          "Path": "/api/payment/{**everything}"
        }
      },
      "Cmskit Service": {
        "ClusterId": "cmskitCluster",
        "Match": {
          "Path": "/api/cmskit/{**everything}"
        }
      },
      "media-picture-route": {
        "ClusterId": "mediaPictureCluster",
        "Match": {
          "Path": "/media-images/{**everything}",
          "Methods": [ "GET" ]
        }
      }
    },
    "Clusters": {
      "accountCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://app-authserver"
          }
        }
      },
      "administrationCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://administration-service"
          }
        }
      },
      "fileCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://file-service"
          }
        }
      },
      "delugeCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://deluge-service"
          }
        }
      },
      "embyCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://emby-service"
          }
        }
      },
      "videoCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://video-service"
          }
        }
      },
      "traktCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://trakt-service"
          }
        }
      },
      "cmskitCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://cmskit-service"
          }
        }
      },
      "mediaPictureCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://file-service"
          }
        }
      }
    }
  }
}
